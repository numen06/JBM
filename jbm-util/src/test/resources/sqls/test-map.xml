<mapper>
    <select id="selectByFileName" >
        select * from sql_initialize
        <where>
            file_name in
            <foreach collection="list" item="item" separator="," open="(" close=")">
                ${item}
            </foreach>
        </where>
    </select>

    <select id="selectByFileName2" >
        select * from sql_initialize
        <where>
            file_name in
            <foreach collection="list" item="item" separator="," open="(" close=")">
                #{item.name}
            </foreach>
        </where>
    </select>

    <select id="selectByFileName3" >
        select * from sql_initialize
        <where>
            file_name = #{name}
        </where>
    </select>


    <select id="selectByFileName4" >
        select * from sql_initialize
        <where>
            file_name = #{student.name}
        </where>
    </select>
</mapper>