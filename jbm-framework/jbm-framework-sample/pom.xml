<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.jbm</groupId>
        <artifactId>jbm-framework</artifactId>
        <version>5.0.0-SNAPSHOT</version>
    </parent>
    <artifactId>jbm-framework-sample</artifactId>
    <packaging>pom</packaging>
    <modules>
        <module>jbm-framework-sample-mweb</module>
        <module>jbm-framework-sample-logger</module>
        <module>jbm-framework-sample-micro-service</module>
        <module>jbm-framework-sample-wechat</module>
        <module>jbm-framework-sample-authserver</module>
        <module>jbm-framework-sample-restapi</module>
        <module>jbm-framework-sample-ui</module>
        <module>jbm-framework-sample-pr</module>
        <module>jbm-framework-sample-swagger</module>
        <module>jbm-framework-sample-mui</module>
        <module>jbm-framework-sample-schedule</module>
        <module>jbm-framework-sample-disconf-web</module>
        <module>jbm-framework-sample-mqtt</module>
        <module>jbm-framework-sample-thrift</module>
        <module>jbm-framework-sample-shop</module>
        <module>jbm-framework-sample-ai</module>
        <module>jbm-framework-sample-leveldb</module>
        <module>jbm-framework-sample-img</module>
        <module>jbm-framework-sample-influxdb</module>
        <module>jbm-framework-sample-mqtt-influxdb</module>
        <module>jbm-framework-sample-mysql</module>
        <module>jbm-framework-sample-jstorm</module>
        <module>jbm-framework-sample-disruptor</module>
        <module>jbm-framework-sample-fastdfs</module>
        <module>jbm-framework-sample-aio</module>
    </modules>
    <dependencies>
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter</artifactId>
        </dependency>
    </dependencies>

    <build>
        <plugins>
            <plugin>
                <artifactId>maven-deploy-plugin</artifactId>
                <configuration>
                    <skip>true</skip>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-surefire-plugin</artifactId>
                <configuration>
                    <skip>true</skip>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>