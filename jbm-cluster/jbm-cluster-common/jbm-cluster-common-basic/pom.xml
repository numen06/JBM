<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <parent>
        <artifactId>jbm-cluster-common</artifactId>
        <groupId>com.jbm</groupId>
        <version>7.1.0-SNAPSHOT</version>
    </parent>
    <modelVersion>4.0.0</modelVersion>

    <artifactId>jbm-cluster-common-basic</artifactId>

    <properties>
        <maven.compiler.source>8</maven.compiler.source>
        <maven.compiler.target>8</maven.compiler.target>
    </properties>
    <dependencies>
        <dependency>
            <groupId>com.jbm</groupId>
            <artifactId>jbm-cluster-api-basic</artifactId>
            <version>${project.version}</version>
        </dependency>
        <!-- SpringBoot Actuator -->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-actuator</artifactId>
        </dependency>

        <!-- SpringCloud Loadbalancer -->
        <dependency>
            <groupId>org.springframework.cloud</groupId>
            <artifactId>spring-cloud-starter-loadbalancer</artifactId>
        </dependency>
        <!-- SpringCloud Bootstrap -->
        <dependency>
            <groupId>org.springframework.cloud</groupId>
            <artifactId>spring-cloud-starter-bootstrap</artifactId>
        </dependency>
        <!-- Jbm Support Mvc -->
        <dependency>
            <groupId>com.jbm</groupId>
            <artifactId>jbm-framework-autoconfigure-mvc</artifactId>
        </dependency>
        <dependency>
            <groupId>com.jbm</groupId>
            <artifactId>jbm-framework-autoconfigure-dictionary</artifactId>
        </dependency>
        <dependency>
            <groupId>com.jbm</groupId>
            <artifactId>jbm-framework-autoconfigure-eventbus</artifactId>
        </dependency>
        <!-- Jbm Spring -->
        <dependency>
            <groupId>com.jbm</groupId>
            <artifactId>jbm-framework-spring</artifactId>
        </dependency>
        <!-- Jbm Swagger -->
        <dependency>
            <groupId>com.jbm</groupId>
            <artifactId>jbm-framework-autoconfigure-swagger</artifactId>
        </dependency>
        <!-- Jbm Redis -->
        <dependency>
            <groupId>com.jbm</groupId>
            <artifactId>jbm-framework-autoconfigure-redis</artifactId>
        </dependency>
        <!-- Transmittable ThreadLocal -->
        <dependency>
            <groupId>com.alibaba</groupId>
            <artifactId>transmittable-thread-local</artifactId>
        </dependency>
        <!-- Hibernate Validator -->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-validation</artifactId>
        </dependency>

        <!-- Jackson -->
        <dependency>
            <groupId>com.fasterxml.jackson.core</groupId>
            <artifactId>jackson-databind</artifactId>
        </dependency>

        <!-- Jwt -->
        <dependency>
            <groupId>io.jsonwebtoken</groupId>
            <artifactId>jjwt</artifactId>
        </dependency>

        <!-- Jaxb -->
        <dependency>
            <groupId>javax.xml.bind</groupId>
            <artifactId>jaxb-api</artifactId>
        </dependency>
        <!-- Java Servlet -->
        <dependency>
            <groupId>javax.servlet</groupId>
            <artifactId>javax.servlet-api</artifactId>
        </dependency>
    </dependencies>
</project>